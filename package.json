{
  "name": "football-updates",
  "version": "1.0.0",
  "author": "Jordi Marti Alamillo",
  "description": "Build a small football (soccer) updates application that follows results from the top 5 European leagues",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --configuration development",
    "mocks": "ng serve --configuration mocks",
    "build": "ng build",
    "deploy": "ng deploy --base-href=/football-updates/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "lint:commit": "commitlint -e",
    "mocks-server": "nodemon --watch mocks --exec dyson mocks-server",
    "mocks:dyson": "dyson ./mocks-server",
    "prepare-husky": "husky install",
    "postinstall": "npm run prepare-husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.0",
    "@angular/common": "16.2.0",
    "@angular/compiler": "16.2.0",
    "@angular/core": "16.2.0",
    "@angular/forms": "16.2.0",
    "@angular/platform-browser": "16.2.0",
    "@angular/platform-browser-dynamic": "16.2.0",
    "@angular/router": "16.2.0",
    "angular-cli-ghpages": "1.0.7",
    "crypto-js": "4.1.1",
    "dyson": "4.1.0",
    "nodemon": "3.0.1",
    "rxjs": "7.8.1",
    "tslib": "2.3.0",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.2",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.2",
    "@angular/compiler-cli": "16.2.0",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/crypto-js": "4.1.2",
    "@types/jasmine": "4.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.49.0",
    "husky": "8.0.3",
    "jasmine-core": "4.6.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "pre-push": "0.1.4",
    "typescript": "5.1.6"
  }
}
